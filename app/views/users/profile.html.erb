<h1><%= @user.name %></h1>

<% @user.books.each do |book| %>
  <li>
    <%= book.title %>
    <%= book.author %>
    <%= book.description %>
    <%= book.progress %>
    <%= link_to 'link', book_path(book.id) %>
   </li>
<% end %>
<%= link_to 'Log out', destroy_user_session_path, method: :delete %>
